#include <iostream>
#include <conio.h>
#include <math.h>
using namespace std;

int main()
{
    int number;
    int f1=0 , f2=1 , f3=1;

    cout << "Enter a number:";
    cin >> number;

    while (number > f3)
    {
        f3 =  f1 + f2;
        f1 = f2;
        f2 = f3;
    }

    if (number == f3)
        cout << "YES!";
    else
        cout << "NO!";
}
